<h3>Create Menu</h3>
<form>
    {{ form_errors(form) }}
    {{ form_errors(form.title) }}
    {{ form_widget(form.title, {'attr': {'placeholder': 'Title', 'class': 'large'}}) }}
    {{ form_rest(form) }}

    <div class="action-bar text-right">
        <button class="button" id="fancy_submit_button">{% trans %}Create{% endtrans %}</button>
        <button class="button secondary" id="fancy_cancel_button">{% trans %}Cancel{% endtrans %}</button>
    </div>
</form>

