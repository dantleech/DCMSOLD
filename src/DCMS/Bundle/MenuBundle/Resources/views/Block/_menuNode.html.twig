<li>
    <a href="{{ node.content ? ep_path(node.content) : node.uri }}">{{ node.label }}</a>
    {% if node.children|length > 0 %}
        <ul>
        {% for child in node.children %}
            {% include "DCMSMenuBundle:Block:_menuNode.html.twig" with {'node': child} %}
        {% endfor %}
        </ul>
    {% endif %}
</li>
